package br.com.ecommerce.catalogoprodutos.model;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;
import javax.persistence.Entity;

@Entity(name = "tbcatalogoprodutos")
@Data
@AllArgsConstructor
@NoArgsConstructor
public class Product {
	
	private Long id;
	
	private String name;
	
	private Double price;

}
